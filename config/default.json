{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8081",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "s3": {
    "awsAccessKeyId": "minioadmin",
    "awsSecretAccessKey": "minioadmin",
    "endPoint": "http://127.0.0.1:9000",
    "forcePathStyle": true,
    "bucket": "3dtiles"
  },
  "3dir": "../3D-Models",
  "source": "NFS",
  "pgboss": "postgres://postgres:postgres@127.0.0.1/s3-key",
  "getS3": "http://tlv-p-netanel-lnx:8080/getS3/"
}
